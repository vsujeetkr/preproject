{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 9 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/accordion_blocks": "^2.0",
        "drupal/admin_toolbar": "^3.1",
        "drupal/blazy": "^2.16",
        "drupal/block_class": "^2.0",
        "drupal/block_field": "^1.0@RC",
        "drupal/block_id": "^2.0@beta",
        "drupal/blockgroup": "^2.0",
        "drupal/bootstrap": "^3.24",
        "drupal/call_now_button": "^1.0",
        "drupal/colorbox": "^2.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/context": "^5.0@RC",
        "drupal/core": "^10",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10",
        "drupal/core-vendor-hardening": "^10",
        "drupal/ctools": "^4.0",
        "drupal/devel": "^5.0",
        "drupal/entity_clone": "^2.0@beta",
        "drupal/entity_field_condition": "^1.3",
        "drupal/field_group": "^3.2",
        "drupal/file_mdm": "^2.6",
        "drupal/flexslider": "^3.0@alpha",
        "drupal/fontawesome": "^2.22",
        "drupal/google_analytics": "^4.0",
        "drupal/image_effects": "^3.4",
        "drupal/image_link_formatter": "^2.0",
        "drupal/image_url_formatter": "^1.0",
        "drupal/imagefield_slideshow": "^2.0",
        "drupal/juicebox": "^4.0@alpha",
        "drupal/libraries": "^4.0",
        "drupal/media_thumbnails": "^1.0@beta",
        "drupal/media_thumbnails_video": "^2.0",
        "drupal/metatag": "^2.0",
        "drupal/node_export": "^1.0@alpha",
        "drupal/oembed_providers": "^2.1",
        "drupal/oembed_thumbnail_chooser": "^1.0@beta",
        "drupal/owlcarousel": "^3.0",
        "drupal/paragraphs": "^1.14",
        "drupal/pathauto": "^1.9",
        "drupal/pcr": "^2.0",
        "drupal/quicktabs": "^3.0@alpha",
        "drupal/schema_metatag": "^3.0",
        "drupal/search_api": "^1.23",
        "drupal/seo_checklist": "^5.1",
        "drupal/simple_popup_blocks": "^3.1",
        "drupal/simple_sitemap": "^4.1",
        "drupal/slick": "^2.6",
        "drupal/slick_views": "^2.6",
        "drupal/smtp": "^1.0",
        "drupal/social_media_links": "^2.8",
        "drupal/taxonomy_menu": "^3.5",
        "drupal/telephone_validation": "^2.3",
        "drupal/token": "^1.10",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_accordion": "^2.0",
        "drupal/views_conditional": "^1.4",
        "drupal/views_exposed_filter_blocks": "^1.1",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/views_slideshow": "^5.0",
        "drupal/views_slideshow_cycle": "^5.0",
        "drupal/views_taxonomy_term_name_depth": "^7.0",
        "drupal/webform": "^6.2@beta",
        "drupal/whatsapp_bubble": "^1.0@beta",
        "drush/drush": "^12.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "patches": {
            "drupal/core": {
                "library definitions have a license with no URL defined": "https://www.drupal.org/files/issues/2023-08-09/3359497-13.patch"
            },
            "drupal/conditional_fields": {
                "Required fields that are not visible are still being require": "https://www.drupal.org/files/issues/2023-05-05/3344587-required_fields_not_visible-15.patch"
            },
            "drupal/simple_popup_blocks": {
                "Uncaught TypeError: $(...).once is not a function with Drupal 10": "https://www.drupal.org/files/issues/2023-08-22/once_deprecation-3350919-25.patch"
            }
        }
    }
}
